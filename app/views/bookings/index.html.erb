<h1>Hello from index bookings</h1>
<h3><%= link_to "Back to home page", root_path %></h3>

<% @bookings.each do |booking| %>
<ul>
  <li><%= booking.start_date %></li>
  <li><%= booking.end_date %></li>
  <%=link_to 'Delete booking', booking_path(booking), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
  <%=link_to 'Edit booking', edit_booking_path(booking)%>
</ul>
<% end %>
